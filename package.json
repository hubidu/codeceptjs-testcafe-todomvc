{
  "name": "codeceptjs-todomvc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "xo": {
    "space": false,
    "semicolon": false,
    "globals": [
      "Feature",
      "Scenario",
      "Before",
      "actor",
      "pause",
      "locate",
      "Given",
      "When",
      "Then"
    ],
    "rules": {
      "new-cap": 0,
      "arrow-parens": 0,
      "indent": [
        "error",
        4
      ]
    }
  },
  "scripts": {
    "test:report-junit": "codeceptjs run --reporter mocha-junit-reporter",
    "test:report-mochawesome": "codeceptjs run --reporter mochawesome"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "codeceptjs": "^2.2.0",
    "mocha-junit-reporter": "^1.22.0",
    "mocha-multi": "^1.1.0",
    "mochawesome": "^3.1.2",
    "testcafe": "^1.20.1"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "xo": "^0.24.0"
  }
}
